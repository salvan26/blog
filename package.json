    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.22.3",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "markdown-to-jsx": "^7.7.1",
    "nanoid": "^5.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.0.2",
